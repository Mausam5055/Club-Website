{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/py/health",
        "dest": "/api/index.py"
      },
      {
        "src": "/api/py/(.*)",
        "dest": "/api/index.py"
      },
      {
        "src": "/api/py/(.*)",
        "dest": "/api/index.py"
      },
      {
        "src": "/test",
        "dest": "/test"
      },
      {
        "src": "/(.*\\.(js|json|css|ico|png|jpg|jpeg|svg|ttf|webp|webm)$)",
        "dest": "/$1"
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_URL": "https://linpack.vercel.app/",
      "PYTHON_VERSION": "3.9"
    }
  }
  